module include/SugarTest-parenthesize

imports
  libstratego-lib
  include/SugarTest


strategies
  io-SugarTest-parenthesize =
    io-wrap(parenthesize-SugarTest)

  parenthesize-SugarTest =
    innermost(SugarTestParenthesize)


rules

  SugarTestParenthesize :
    TupleBranch(t_0, t_1) -> TupleBranch(Parenthetical(t_0), t_1)
    where <(?TupleBranch(_, _) + fail)> t_0


signature
  constructors
    Parenthetical : Unknown -> Unknown

